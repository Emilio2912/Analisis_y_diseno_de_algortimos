def fibonacci_iterativo(n):
    a, b = 0, 1
    for _ in range(n):
        print(a)
        a, b = b, a + b

n = 20  
print(f"Imprimiendo serie hasta posición {n}")
valor = fibonacci_iterativo(n)





#factorial 
print("Ingrese el número al cual desea calcular el factorial")
num = int (input())
fact=1
for i in range(1, num+1):
    fact*=i
print ("El factorial de " , num,"es igual a: ", fact)
